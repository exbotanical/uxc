{
  "name": "@uxc/client",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Matthew Zito (goldmund) <matthewtzito@gmail.com>",
  "scripts": {
    "build": "vite build",
    "check": "pnpm lint && pnpm lint:css && pnpm typecheck",
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{ts,tsx,md,css,scss}\"",
    "lint": "eslint --ext .ts,.js,.tsx,.jsx,.json . --fix",
    "lint:css": "stylelint \"src/**/*.{css,scss,ts,tsx}\" --fix",
    "serve": "vite preview --port 3000",
    "test:cy": "cross-env VITE_CY_TEST=true pnpm build && start-server-and-test serve http-get://localhost:3000/ \"cypress open\"",
    "test:cy:ci": "cross-env VITE_CY_TEST=true pnpm build && start-server-and-test serve http-get://localhost:3000/ \"cypress run --env CY_CI=true\"",
    "test:cy:dev": "cross-env VITE_CY_TEST=true CYPRESS_BASE_URL=http://localhost:3000 CYPRESS_STAGE=dev start-server-and-test dev http-get://localhost:3000/ \"cypress open\"",
    "test:unit": "jest",
    "test:unit:dev": "jest --watchAll",
    "test": "npm run test:unit && npm run test:cy:ci",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.5.7",
    "framer-motion": "^5.6.0",
    "graphql": "^16.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet-async": "^1.2.3",
    "react-redux": "7.2.5",
    "react-responsive": "9.0.0-beta.6",
    "react-router-dom": "6.2.1",
    "react-router-hash-link": "^2.4.3",
    "redux": "4.1.1",
    "styled-components": "^5.3.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@cypress/vite-dev-server": "^2.2.2",
    "@magister_zito/eslint-config-react": "^0.9.0",
    "@magister_zito/stylelint-config": "^0.2.1",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@types/jest": "^27.4.0",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "5.3.2",
    "@types/react-router-hash-link": "^2.4.5",
    "@types/styled-components": "^5.1.22",
    "@types/uuid": "8.3.1",
    "@uxc/common": "workspace:^1.0.0",
    "@vitejs/plugin-react": "1.0.1",
    "autoprefixer": "10.3.7",
    "axe-core": "^4.3.5",
    "cross-env": "^7.0.3",
    "cypress": "9.5.2",
    "cypress-axe": "^0.14.0",
    "cypress-real-events": "^1.7.0",
    "dotenv": "^14.2.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "jest": "^27.4.7",
    "jest-styled-components": "^7.0.8",
    "postcss": "8.3.9",
    "prettier": "2.4.1",
    "sass": "1.42.0",
    "start-server-and-test": "^1.14.0",
    "stylelint": "13",
    "stylelint-config-recommended": "^7.0.0",
    "ts-jest": "^27.1.3",
    "typescript": "4.4.2",
    "vite": "2.5.1",
    "vite-plugin-svg-icons": "^1.1.0"
  }
}
